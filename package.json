{
  "name": "backoffice",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --optimization --extra-webpack-config webpack.extra.js",
    "build:ci": "ng build --aot --vendor-chunk --common-chunk --delete-output-path --optimization --extra-webpack-config webpack.extra.js",
    "build:stats": "ng build --stats-json --prod --aot --vendor-chunk --common-chunk --delete-output-path --optimization --extra-webpack-config webpack.extra.js",
    "test": "ng test",
    "test:ci": "ng test --config karma.conf.ci.js --code-coverage --progress=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:pre-ci": "./node_modules/webdriver-manager/bin/webdriver-manager update --versions.chrome=2.43",
    "e2e:ci": "ng e2e --config protractor.conf.ci.js --progress=false --webdriver-update false",
    "e2e:post-ci": "cat ./reports/e2e.json | ./node_modules/.bin/cucumber-junit > ./reports/e2e.xml",
    "doc:build": "compodoc -p src/tsconfig.app.json",
    "doc:serve": "compodoc -s",
    "doc:buildandserve": "compodoc -p src/tsconfig.app.json -s",
    "analyze:es5": "webpack-bundle-analyzer dist/stats-es5.json",
    "analyze:es2015": "webpack-bundle-analyzer dist/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
