    "@types/history": "^4.7.2",
    "@types/jasmine": "^2.5.38",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.29",
    "@types/node": "^12.0.10",
    "codelyzer": "^5.0.1",
    "cucumber-junit": "^1.7.1",
    "jasmine-core": "~2.5.2",
    "jasmine-reporters": "latest",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
