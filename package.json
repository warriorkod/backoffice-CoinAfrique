    "ng2-image-compress": "^7.0.7",
    "ng2-img-max": "2.1.6",
    "ngx-build-plus": "^8.0.3",
    "ngx-img-fallback": "1.2.0",
    "ngx-loading": "^1.0.15",
    "ngx-moment": "^3.4.0",
