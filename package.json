    "node-sass": "^4.12.0",
    "npm-check": "^5.9.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
